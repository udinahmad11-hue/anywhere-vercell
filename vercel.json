{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/proxy"
    },
    {
      "source": "/cors",
      "destination": "/api/cors"
    },
    {
      "source": "/cors/(.*)",
      "destination": "/api/cors"
    },
    {
      "source": "/proxy/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/api/[[...route]]"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" }
      ]
    }
  ]
}
