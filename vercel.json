{
  "rewrites": [
    {
      "source": "/api/proxy/:url+",
      "destination": "/api/proxy"
    },
    {
      "source": "/proxy/:url+",
      "destination": "/api/proxy"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "functions": {
    "api/proxy.js": {
      "maxDuration": 10
    }
  }
}
